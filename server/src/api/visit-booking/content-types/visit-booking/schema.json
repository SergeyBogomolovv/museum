{
  "kind": "collectionType",
  "collectionName": "visit_bookings",
  "info": {
    "singularName": "visit-booking",
    "pluralName": "visit-bookings",
    "displayName": "VisitBooking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "email": {
      "type": "email"
    },
    "startTime": {
      "type": "datetime"
    },
    "visitorsCount": {
      "type": "integer"
    },
    "bookingCode": {
      "type": "uid"
    },
    "bookingStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "cancelled"
      ],
      "default": "pending"
    },
    "name": {
      "type": "string"
    },
    "wishes": {
      "type": "text"
    },
    "rejectReason": {
      "type": "text"
    },
    "isAdminEmailSent": {
      "type": "boolean",
      "default": false
    },
    "isUserEmailSent": {
      "type": "boolean",
      "default": false
    }
  }
}
